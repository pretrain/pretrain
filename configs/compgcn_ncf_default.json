{
    "model": "compgcn_ncf_gmf",
    "model_comments": "'gmf', 'mlp', 'ncf', and 'ncf_gmf'",
    "config_id": "default",
    "root_dir": "../",
    "pre_train": 0,
    "dataset": "ml_1m",
    "data_split": "leave_one_out",
    "validate": 1,
    "emb_dim": 64,
    "base_dim": 64,
    "late_dim": 64,
    "n_bases": 10,
    "num_negative": 4,
    "batch_size": 1000,
    "dropout": 0.0,
    "metrics": ["ndcg_at_k", "precision_at_k", "recall_at_k", "map_at_k"],
    "validate_metric": "ndcg_at_k@10",
    "device": "gpu",
    "optimizer": "adam",
    "lr": 1e-3,
    "reg": 1e-3,
    "activator": "tanh",
    "opn": "sub",
    "max_epoch": 100,
    "result_file": "compgcn_ncf_result.csv",
    "log_dir": "logs/",
    "result_dir": "results/",
    "checkpoint_dir": "checkpoints/",
    "dataset_dir": "datasets/",
    "run_dir": "runs/",
    "gmf_config": {"name": "gmf", "save_name": "gmf.model"},
    "mlp_config": {"name": "mlp", "n_layers": 3, "save_name": "mlp.model"},
    "neumf_config": {"name": "ncf", "save_name": "ncf.model"},
    "compgcn_config": {
        "save_name": "compgcn.model"
    }
}

